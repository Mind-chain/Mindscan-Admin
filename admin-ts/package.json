  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p .",
    "copy-files": "copyfiles -u 1 public/**/*.css public/**/*.png public/**/*.svg dist/public/",
    "generate": "yarn prisma generate --schema prisma/contracts-info.prisma && yarn prisma generate --schema prisma/admin-schema.prisma",
    "start:prod": "node ./dist/index.js",
    "start": "yarn ts-node src/index.ts",
    "watch": "yarn nodemon src/index.ts",
    "wrong_start": "tsc && node dist/index.js",
    "gstart": "yarn generate && yarn start",
    "build:prod": "yarn clean && yarn generate && yarn build && yarn copy-files",
    "lint": "yarn eslint . --ext .ts && yarn prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
